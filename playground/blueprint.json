{
    "$schema": "https://playground.wordpress.net/blueprint-schema.json",
    "landingPage": "/wp-admin/edit.php?post_type=page",
    "steps": [
        {
			"step": "login",
			"username": "admin"
		},
        {
            "step": "setSiteOptions",
            "options": {
                "blogname": "WP FAQ Block"
            }
        },
        {
            "step": "installPlugin",
            "pluginData": {
                "resource": "url",
                "url": "https://github-proxy.com/proxy/?repo=BhargavBhandari90/wp-faq-block"
            }
        },
        {
            "step": "activatePlugin",
            "pluginName": "Wp Faq Block",
            "pluginPath": "/wordpress/wp-content/plugins/wp-faq-block"
        },
        {
            "step": "importWxr",
            "file": {
                "resource": "url",
                "url": "https://github.com/BhargavBhandari90/wp-faq-block/blob/main/playground/wordpress.xml"
            }
        }
    ]
}